{
  "manifest_version": 2,

  "name": "My Jira Plugin",
  "description": "This extension provides controls to JIRA",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab","alarms",
    "https://ajax.googleapis.com/",
    "https://jira.ops.aol.com/"
  ]
}
